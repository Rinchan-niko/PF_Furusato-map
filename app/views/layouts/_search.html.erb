<div class="container">
  <div class="row">
    <div class="col-9 offset-2">
      <div>
        <%= form_with url: search_posts_path, method: :get, local: true do |f| %>
          <%= f.text_field :search, class: 'form-control', placeholder: "タイトル検索"  %>
          <%= f.button :type => "submit" do %>
            <i class="fas fa-search"></i>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>